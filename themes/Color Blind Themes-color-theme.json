{
	"name": "Color Blind Theme",
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"version": "1.0.0",
	"publisher": "Goofygoobers",
	"repository": {
		"type": "git",
		"url": "https://github.com/goofygoobers/color-blind-vscode-theme.git"
	},
	"colors": {
    "foreground": "#e6e6e6",
    "editorLink.activeForeground": "#ff5c57",
    "editor.background": "#20212b",
    "editorCursor.foreground": "#97979b",
    "editor.findMatchHighlightBackground": "#54555e",
    "editor.findRangeHighlightBackground": "#54555e",
    "editor.foreground": "#e6e6e6",
    "editor.hoverHighlightBackground": "#54555e",
    "editorIndentGuide.background": "#616263",
    "editor.lineHighlightBackground": "#1c1e27",
    "editorLineNumber.foreground": "#616263",
    "editor.rangeHighlightBackground": "#54555e",
    "editor.selectionHighlightBackground": "#54555e",
    "editorWhitespace.foreground": "#616263",
    "editor.wordHighlightBackground": "#54555e",
    "editor.wordHighlightStrongBackground": "#54555e",
    "peekView.border": "#5EFFA0",
    "peekViewEditor.background": "#20212b",
    "list.activeSelectionBackground": "#54555e",
    "list.hoverBackground": "#54555e",
    "statusBar.background": "#2E313D",
    "statusBar.foreground": "#6C6E75",
    "statusBar.debuggingBackground": "#6C6E75",
    "statusBar.noFolderBackground": "#6C6E75",
    "terminal.ansiBlack": "#20212b",
    "terminal.ansiBlue": "#5DBBFF",
    "terminal.ansiBrightBlack": "#20212b",
    "terminal.ansiBrightBlue": "#5DBBFF",
    "terminal.ansiBrightCyan": "#9aedfe",
    "terminal.ansiBrightGreen": "#5EFFA0",
    "terminal.ansiBrightMagenta": "#ff6ac1",
    "terminal.ansiBrightRed": "#ff5c57",
    "terminal.ansiBrightWhite": "#e6e6e6",
    "terminal.ansiBrightYellow": "#FFC357",
    "terminal.ansiCyan": "#9aedfe",
    "terminal.ansiGreen": "#5EFFA0",
    "terminal.ansiMagenta": "#ff6ac1",
    "terminal.ansiRed": "#ff5c57",
    "terminal.ansiWhite": "#e6e6e6",
    "terminal.ansiYellow": "#FFC357",
    "activityBar.background": "#20212b",
    "sideBar.background": "#282a36",
    "tab.inactiveBackground": "#282a36",
    "tab.activeBackground": "#1c1e27",
    "editorGroup.dropBackground": "#232631",
    "editorGroupHeader.tabsBackground": "#232631",
    "titleBar.activeBackground": "#20212b"
  },
  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "background": "#20212b",
        "foreground": "#e6e6e6"
      }
    },
    {
      "name": "Strong",
      "scope": "strong, markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Emphasis",
      "scope": "emphasis, markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Comment",
      "scope": "comment, punctuation.definition.comment, string.quoted.docstring",
      "settings": {
        "foreground": "#616263",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type",
      "settings": {
        "foreground": "#FFC357"
      }
    },
    {
      "name": "Type",
      "scope": "storage.type.primitive, support.type, entity.name.type, keyword.type, keyword.other.unit",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "String",
      "scope": "string, meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#5EFFA0"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#5EFFA0"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric, support.constant",
      "settings": {
        "foreground": "#5EFFA0"
      }
    },
    {
      "name": "Null",
      "scope": "constant.language.null",
      "settings": {
        "foreground": "#5EFFA0"
      }
    },
    {
      "name": "Undefined",
      "scope": "constant.language.undefined",
      "settings": {
        "foreground": "#5EFFA0"
      }
    },
    {
      "name": "Function definition",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#5DBBFF"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function, meta.function-call, support.function, keyword.other.special-method, meta.require, variable.language.super, keyword.other.get, keyword.other.set",
      "settings": {
        "foreground": "#5DBBFF"
      }
    },
    {
      "name": "Identifier",
      "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "name": "Constants",
      "scope": "variable.other.constant.property",
      "settings": {
        "foreground": "#FFC357"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape",
        "constant.other"
      ],
      "settings": {
        "foreground": "#ffd3d9"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class entity.name.type.module",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property",
      "settings": {
        "foreground": "#FFC357"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#FFC357"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, modifier, storage.modifier, support.type.object, constant.language, variable.language.this, variable.language.special.self",
      "settings": {
        "foreground": "#ff1ea1"
      }
    },
    {
      "name": "Storage Modifier [e.g. class, extends, static] (JS",
      "scope": "storage.modifier, storage.type.class.js, variable.language.this, variable.language.this.js",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#FFC357"
      }
    },
    {
      "name": "Diff insertion",
      "scope": "markup.inserted.diff, meta.diff.header.to-file",
      "settings": {
        "foreground": "#5EFFA0",
        "background": "#616263"
      }
    },
    {
      "name": "Diff deletion",
      "scope": "markup.deleted.diff, meta.diff.header.from-file",
      "settings": {
        "foreground": "#ff5c57",
        "background": "#616263"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#FFC357"
      }
    },
    {
      "name": "C# method call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#5DBBFF"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#FFC357"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "C# preprocessor",
      "scope": "source.cs meta.preprocessor",
      "settings": {
        "foreground": "#616263"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "CSS id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#FFC357"
      }
    },
    {
      "name": "CSS property",
      "scope": "source.css support.type.property-name",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FFC357",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML tag expression",
      "scope": "punctuation.section.embedded",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#5DBBFF"
      }
    },
    {
      "name": "Java package & import",
      "scope": "source.java storage.modifier.package, source.java storage.modifier.import",
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "name": "Java annotation",
      "scope": "source.java storage.type.annotation",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Java method call",
      "scope": "source.java meta.method-call meta.method",
      "settings": {
        "foreground": "#5DBBFF"
      }
    },
    {
      "name": "Java inner-class variable",
      "scope": "source.java meta.method-call meta.inner-class meta.method",
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "name": "Java inner-class method call",
      "scope": "source.java meta.method-call meta.inner-class meta.method meta.method-call meta.method",
      "settings": {
        "foreground": "#5DBBFF"
      }
    },
    {
      "name": "Java inner-class method",
      "scope": "source.java meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#5DBBFF"
      }
    },
    {
      "name": "Java null",
      "scope": "source.java constant.language",
      "settings": {
        "foreground": "#5EFFA0"
      }
    },
    {
      "name": "Async",
      "scope": "source.js storage.modifier.async",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#5DBBFF"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference, markup.underline.link.image",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Markdown block quote",
      "scope": "text.html.markdown markup.quote",
      "settings": {
        "foreground": "#9aedfe"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#FFC357"
      }
    },
    {
      "name": "Markdown code",
      "scope": "text.html.markdown markup.inline.raw",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#ff5c57",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Python import *",
      "scope": "source.python keyword.operator.arithmetic",
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "name": "Python def",
      "scope": "source.python meta.function storage.type.function",
      "settings": {
        "foreground": "#ff6ac1"
      }
    },
    {
      "name": "Python function arguments",
      "scope": "source.python meta.function-call.arguments",
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
    {
      "name": "Python function decorators",
      "scope": "source.python meta.function entity.name.function.decorator",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Python True & False & None",
      "scope": "source.python constant.language",
      "settings": {
        "foreground": "#5EFFA0"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#ff5c57"
      }
    },
    {
      "name": "Some keywords like const, let, var, extends in Javascript will use this",
      "scope": "storage.type",
      "settings": {
        "foreground": "#F9CB63"
      }
    },
    {
      "name": "To differentiate the signals <> from tags and make tags shinier",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#943431"
      }
    },
    {
      "name": "Function calls can have an actual collor",
      "scope": "variable.function",
      "settings": {
        "foreground": "#C95297"
      }
    },
    {
      "name": "Parameters from functions, like function about(thisParameterHere)",
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#AC8C44"
      }
    },
    {
      "name": "Keywords like import and export",
      "scope": "keyword.operator.module",
      "settings": {
        "foreground": "#848BB2"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "background": "#3D88AF"
      }
    },
    {
      "name": "Keys in object literals",
      "scope": "string.unquoted.label",
      "settings": {
        "foreground": "#5AC143"
      }
    },
    {
      "name": "Separator in key: value for object literals",
      "scope": "punctuation.separator.key-value.js",
      "settings": {
        "foreground": "#428D30"
      }
    },
    {
      "name": "Curly braces that doesnt need highlight",
      "scope": "meta.brace.curly.begin.js, meta.brace.curly.end.js, meta.brace.round.begin.js, meta.brace.round.end.js, meta.brace.square.begin.js, meta.brace.square.end.js",
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "scope": "string.interpolated.js",
      "settings": {
        "background": "#ffffff"
      }
    }
  ]
}